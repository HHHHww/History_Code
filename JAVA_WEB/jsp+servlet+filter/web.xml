<?xml version="1.0" encoding="UTF-8"?>
<web-app>

    <servlet>
        <servlet-name>welcome</servlet-name>
        <servlet-class>test.login_servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>welcome</servlet-name>
        <url-pattern>/login11.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>web_servlet</servlet-name>
        <servlet-class>test.web_servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>web_servlet</servlet-name>
        <url-pattern>/web_servlet</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>login_filter</filter-name>
        <filter-class>test.login_filter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>login_filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <!--   servlet结束 -->

</web-app>
